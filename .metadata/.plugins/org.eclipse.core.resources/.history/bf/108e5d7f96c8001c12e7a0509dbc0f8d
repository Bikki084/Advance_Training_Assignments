package com.problem_5.problem_statement_5_2;

import java.util.Scanner;

public class SplitMethod {
	public static void main(String[] args) {

		String txt= (" 23  +  45  -  (  343  /  12  ) ");
		txt = txt.trim();
		String[] w=txt.split("\\s");
		
		for(String w1:w){  
			System.out.println(w1.trim()); 
			//System.out.println(" ");
		}
	}

}